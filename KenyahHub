if game.PlaceId == 1597763879 or game.PlaceId == 335760407 then

    -- AntiCheat Desactivado Message
    local msg = Instance.new("TextLabel")
    msg.Size = UDim2.new(0, 300, 0, 50)
    msg.Position = UDim2.new(0.5, -150, 0.1, 0)
    msg.AnchorPoint = Vector2.new(0.5, 0)
    msg.Text = " Kenya Se follo al AntiCheat "
    msg.TextSize = 22
    msg.TextColor3 = Color3.fromRGB(255, 255, 255)
    msg.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    msg.BackgroundTransparency = 0.3
    msg.BorderSizePixel = 0
    msg.Parent = game:GetService("CoreGui")

    task.delay(2, function()
        msg:Destroy()
    end)

    
-- GUI con menú principal y submenús (6 casillas + minimizar + cerrar)

-- Variables
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

-- Pantalla principal
local screenGui = Instance.new("ScreenGui", playerGui)
screenGui.Name = "MainGui"
screenGui.ResetOnSpawn = false

-- Frame principal
local mainFrame = Instance.new("Frame", screenGui)
mainFrame.Size = UDim2.new(0, 400, 0, 300)
mainFrame.Position = UDim2.new(0.5, -200, 0.5, -150)
mainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
mainFrame.BorderSizePixel = 0
mainFrame.Visible = true
mainFrame.Name = "MainFrame"

-- UICorner para bordes redondeados
local corner = Instance.new("UICorner", mainFrame)
corner.CornerRadius = UDim.new(0, 12)

-- Minimizar botón
local minimize = Instance.new("TextButton", mainFrame)
minimize.Size = UDim2.new(0, 30, 0, 30)
minimize.Position = UDim2.new(1, -70, 0, 10)
minimize.Text = "-"
minimize.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
minimize.TextColor3 = Color3.fromRGB(255, 255, 255)

-- Cerrar botón
local close = Instance.new("TextButton", mainFrame)
close.Size = UDim2.new(0, 30, 0, 30)
close.Position = UDim2.new(1, -35, 0, 10)
close.Text = "X"
close.BackgroundColor3 = Color3.fromRGB(120, 40, 40)
close.TextColor3 = Color3.fromRGB(255, 255, 255)

-- Funciones de botones
close.MouseButton1Click:Connect(function()
	screenGui:Destroy()
end)

local minimized = false
minimize.MouseButton1Click:Connect(function()
	minimized = not minimized
	for _, v in ipairs(mainFrame:GetChildren()) do
		if v:IsA("TextButton") and v.Name ~= "Minimize" and v.Name ~= "Close" then
			v.Visible = not minimized
		end
	end
end)

-- Marco de botones de menú
local menuFrame = Instance.new("Frame", mainFrame)
menuFrame.Position = UDim2.new(0, 20, 0, 60)
menuFrame.Size = UDim2.new(0, 100, 0, 210)
menuFrame.BackgroundTransparency = 1

-- Marco de contenido
local contentFrame = Instance.new("Frame", mainFrame)
contentFrame.Position = UDim2.new(0, 130, 0, 60)
contentFrame.Size = UDim2.new(0, 250, 0, 210)
contentFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
Instance.new("UICorner", contentFrame).CornerRadius = UDim.new(0, 8)

-- Botón volver
local backButton = Instance.new("TextButton", contentFrame)
backButton.Size = UDim2.new(0, 60, 0, 25)
backButton.Position = UDim2.new(0, 10, 1, -35)
backButton.Text = "< Volver"
backButton.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
backButton.TextColor3 = Color3.fromRGB(255, 255, 255)
backButton.Visible = false

-- Contenido de cada casilla
local contents = {}
for i = 1, 6 do
	local content = Instance.new("TextLabel", contentFrame)
	content.Size = UDim2.new(1, -20, 1, -20)
	content.Position = UDim2.new(0, 10, 0, 10)
	content.BackgroundTransparency = 1
	content.TextColor3 = Color3.fromRGB(255, 255, 255)
	content.TextWrapped = true
	content.TextScaled = true
	content.Visible = false
	contents[i] = content
end

-- Contenido específico de la casilla 1 (info del jugador)
contents[1].Text = "** Jugador: **" .. player.Name .. "\n** Executor: Synapse X (ejemplo)**"
for i = 2, 6 do
	contents[i].Text = "** Casilla** " .. i .. "\n(Soon)"
end

-- Crear los botones del menú
for i = 1, 6 do
	local button = Instance.new("TextButton", menuFrame)
	button.Size = UDim2.new(1, 0, 0, 30)
	button.Position = UDim2.new(0, 0, 0, (i - 1) * 35)
	button.Text = "Casilla " .. i
	button.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
	button.TextColor3 = Color3.fromRGB(255, 255, 255)
	button.Name = "Casilla" .. i

	button.MouseButton1Click:Connect(function()
		-- Mostrar solo el contenido correspondiente
		for j = 1, 6 do
			contents[j].Visible = (j == i)
		end
		backButton.Visible = true
	end)
end

-- Botón volver regresa al menú principal
backButton.MouseButton1Click:Connect(function()
	for i = 1, 6 do
		contents[i].Visible = false
	end
	backButton.Visible = false
end)
